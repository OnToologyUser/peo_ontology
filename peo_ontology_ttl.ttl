@prefix : <https://w3id.org/peo#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix peo: <https://w3id.org/peo#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix peo1: <https://w3id.org/peo#peo:> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <https://w3id.org/peo#> .

<https://w3id.org/peo#> rdf:type owl:Ontology ;
                         owl:versionIRI <https://w3id.org/peo#> ;
                         rdfs:label "PEO ontology: an ontology for prompt engineering and large language models." ;
                         owl:versionInfo 1.0 ;
                         peo:creator "Simone Gramegna" ;
                         peo:description "The PEO ontology is a modern ontology that covers state-of-art prompt engineering techniques and large language models. The ontology is designed to support students, researchers and developers who want to know more about prompt engineering, applying prompting techniques in solving tasks." ;
                         peo:language "english" .

#################################################################
#    Annotation properties
#################################################################

###  https://w3id.org/peo#creator
peo:creator rdf:type owl:AnnotationProperty .


###  https://w3id.org/peo#description
peo:description rdf:type owl:AnnotationProperty .


###  https://w3id.org/peo#language
peo:language rdf:type owl:AnnotationProperty .


###  https://w3id.org/peo#paper
peo:paper rdf:type owl:AnnotationProperty .


###  https://w3id.org/peo#reference
peo:reference rdf:type owl:AnnotationProperty .


###  https://w3id.org/peo#title
peo:title rdf:type owl:AnnotationProperty .


###  https://w3id.org/peo#website
peo:website rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.w3.org/2002/07/owl#topObjectProperty
owl:topObjectProperty rdfs:domain peo:task ;
                      rdfs:range peo:capability .


###  https://w3id.org/peo#based_on
peo:based_on rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdf:type owl:FunctionalProperty ;
             rdfs:domain peo:large_language_model ;
             rdfs:range peo:large_language_model ;
             rdfs:label "based_on" ;
             peo:description "Relationships between to two large language models in which a large language model uses another large language model as base." .


###  https://w3id.org/peo#belongs_to
peo:belongs_to rdf:type owl:ObjectProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain peo:organization ;
               rdfs:range peo:organization ;
               rdfs:label "belongs_to" ;
               peo:description "Relationships between two organizations, in which an organization belongs to an another organization" .


###  https://w3id.org/peo#develops
peo:develops rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdf:type owl:TransitiveProperty ;
             rdfs:domain peo:organization ;
             rdfs:range peo:Base_model ,
                        peo:large_language_model ;
             owl:propertyDisjointWith peo:evolves ;
             rdfs:label "develops" ;
             peo:description "Relationship between an organization and a large language model or an architecture where the organization develops that technology" .


###  https://w3id.org/peo#evolves
peo:evolves rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdf:type owl:TransitiveProperty ;
            rdfs:domain peo:large_language_model ;
            rdfs:range peo:large_language_model ;
            rdfs:label "evolves" ;
            peo:description "Relationships between two model in which a model evolves, it is the next version of the other model." .


###  https://w3id.org/peo#has_capability
peo:has_capability rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf owl:topObjectProperty ;
                   rdf:type owl:AsymmetricProperty ;
                   rdfs:domain peo:large_language_model ;
                   rdfs:range peo:capability ;
                   rdfs:label "has_capability" ;
                   peo:description "Relationship between a large language model and a capability where the model has that capability." .


###  https://w3id.org/peo#has_model_architecture
peo:has_model_architecture rdf:type owl:ObjectProperty ,
                                    owl:FunctionalProperty ,
                                    owl:AsymmetricProperty ;
                           rdfs:domain peo:large_language_model ;
                           rdfs:range peo:Base_model ;
                           rdfs:label "has_model_architecture" ;
                           peo:description "Relationship between a large language model and a basic model, where the model is the component of  the large language model." .


###  https://w3id.org/peo#has_prompt_input
peo:has_prompt_input rdf:type owl:ObjectProperty ;
                     owl:inverseOf peo:has_prompt_output ;
                     rdfs:domain peo:prompt_output ;
                     rdfs:range peo:prompting_technique .


###  https://w3id.org/peo#has_prompt_output
peo:has_prompt_output rdf:type owl:ObjectProperty ,
                               owl:FunctionalProperty ;
                      rdfs:domain peo:prompting_technique ;
                      rdfs:range peo:prompt_output ;
                      rdfs:label "has_prompt_output" .


###  https://w3id.org/peo#produced_by
peo:produced_by rdf:type owl:ObjectProperty ;
                owl:inverseOf peo:produces_output ;
                rdf:type owl:FunctionalProperty ;
                rdfs:domain peo:prompt_output ;
                rdfs:range peo:large_language_model ;
                rdfs:label "produced_by" .


###  https://w3id.org/peo#produces_output
peo:produces_output rdf:type owl:ObjectProperty ;
                    rdfs:domain peo:large_language_model ;
                    rdfs:range peo:prompt_output .


###  https://w3id.org/peo#requires_capability
peo:requires_capability rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf owl:topObjectProperty ;
                        rdf:type owl:AsymmetricProperty ;
                        rdfs:label "requires_capability" ;
                        peo:description "Relationship between a task to solve and a capaiblity, the task in order to be solved requires a specific capability." .


###  https://w3id.org/peo#solved_by
peo:solved_by rdf:type owl:ObjectProperty ;
              owl:inverseOf peo:solves_task ;
              rdf:type owl:FunctionalProperty ;
              rdfs:domain peo:task ;
              rdfs:range peo:prompting_technique ;
              rdfs:label "solved_by" .


###  https://w3id.org/peo#solves_task
peo:solves_task rdf:type owl:ObjectProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain peo:prompting_technique ;
                rdfs:range peo:task .


#################################################################
#    Data properties
#################################################################

###  https://w3id.org/peo#has_body
peo:has_body rdf:type owl:DatatypeProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain peo:prompting_technique ;
             rdfs:range xsd:string ;
             rdfs:label "has_body" .


###  https://w3id.org/peo#has_description
peo:has_description rdf:type owl:DatatypeProperty ;
                    rdfs:domain peo:task ;
                    rdfs:range xsd:string ;
                    rdfs:label "has_description" .


###  https://w3id.org/peo#has_number_parameters
peo:has_number_parameters rdf:type owl:DatatypeProperty ;
                          rdfs:subPropertyOf owl:topDataProperty ;
                          rdf:type owl:FunctionalProperty ;
                          rdfs:domain peo:large_language_model ;
                          rdfs:range xsd:long ;
                          rdfs:label "has_number_parameters" ;
                          peo:description "Number of parameters of a large language model." .


###  https://w3id.org/peo#has_output_value
peo:has_output_value rdf:type owl:DatatypeProperty ,
                              owl:FunctionalProperty ;
                     rdfs:domain peo:prompt_output ;
                     rdfs:range xsd:string ;
                     rdfs:label "has_output_value" .


###  https://w3id.org/peo#has_release_year
peo:has_release_year rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf owl:topDataProperty ;
                     rdf:type owl:FunctionalProperty ;
                     rdfs:domain peo:large_language_model ;
                     rdfs:range xsd:integer ;
                     rdfs:label "has_release_year" ;
                     peo:description "Release year of a model or a model architecture." .


