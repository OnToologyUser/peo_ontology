@prefix : <https://w3id.org/peo#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <https://w3id.org/peo#> .

<https://w3id.org/peo#> rdf:type owl:Ontology ;
                         owl:versionIRI <https://w3id.org/peo#> ;
                         rdfs:label "PEO ontology: an ontology for prompt engineering and large language models." ;
                         owl:versionInfo 1.0 ;
                         :creator "Simone Gramegna" ;
                         :description "The PEO ontology is a modern ontology that covers state-of-art prompt engineering techniques and large language models. The ontology is designed to support students, researchers and developers who want to know more about prompt engineering, applying prompting techniques in solving tasks." ;
                         :language "english" .

#################################################################
#    Annotation properties
#################################################################

###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


###  https://w3id.org/peo#creator
:creator rdf:type owl:AnnotationProperty .


###  https://w3id.org/peo#description
:description rdf:type owl:AnnotationProperty .


###  https://w3id.org/peo#language
:language rdf:type owl:AnnotationProperty .


###  https://w3id.org/peo#paper
:paper rdf:type owl:AnnotationProperty .


###  https://w3id.org/peo#reference
:reference rdf:type owl:AnnotationProperty .


###  https://w3id.org/peo#title
:title rdf:type owl:AnnotationProperty .


###  https://w3id.org/peo#website
:website rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.w3.org/2002/07/owl#topObjectProperty
owl:topObjectProperty rdfs:domain :task ;
                      rdfs:range :capability .


###  https://w3id.org/peo#based_on
:based_on rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          rdfs:domain :large_language_model ;
          rdfs:range :large_language_model ;
          rdfs:label "based_on" ;
          :description "Relationships between to two large language models in which a large language model uses another large language model as base." .


###  https://w3id.org/peo#belongs_to
:belongs_to rdf:type owl:ObjectProperty ;
            rdfs:domain :organization ;
            rdfs:range :organization ;
            rdfs:label "belongs_to" ;
            :description "Relationships between two organizations, in which an organization belongs to an another organization" .


###  https://w3id.org/peo#developed_by
:developed_by rdf:type owl:ObjectProperty ;
              owl:inverseOf :develops ;
              rdfs:domain :large_language_model ;
              rdfs:range :organization ;
              rdfs:label "developed_by" .


###  https://w3id.org/peo#develops
:develops rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          rdfs:domain :organization ;
          rdfs:range :Base_model ,
                     :large_language_model ;
          rdfs:label "develops" ;
          :description "Relationship between an organization and a large language model or an architecture where the organization develops that technology" .


###  https://w3id.org/peo#evolves
:evolves rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty ;
         rdf:type owl:TransitiveProperty ;
         rdfs:domain :large_language_model ;
         rdfs:range :large_language_model ;
         rdfs:label "evolves" ;
         :description "Relationships between two model in which a model evolves, it is the next version of the other model." .


###  https://w3id.org/peo#generates_response
:generates_response rdf:type owl:ObjectProperty ;
                    owl:inverseOf :response_generated_using ;
                    rdfs:domain :large_language_model ;
                    rdfs:range :response ;
                    rdfs:label "generates_response" ;
                    :description "Relation between a larga language model and a response, where the model generates the response." .


###  https://w3id.org/peo#has_capability
:has_capability rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                rdfs:domain :large_language_model ;
                rdfs:range :capability ;
                rdfs:label "has_capability" ;
                :description "Relationship between a large language model and a capability where the model has that capability." .


###  https://w3id.org/peo#has_context
:has_context rdf:type owl:ObjectProperty ;
             owl:inverseOf :has_prompt ;
             rdfs:domain :prompt ;
             rdfs:range :chat ;
             rdfs:label "has_context" ;
             :description "Relation between a prompt and a chat, where the chat is the context of the prompt." .


###  https://w3id.org/peo#has_model_architecture
:has_model_architecture rdf:type owl:ObjectProperty ;
                        rdfs:domain :large_language_model ;
                        rdfs:range :Base_model ;
                        rdfs:label "has_model_architecture" ;
                        :description "Relationship between a large language model and a basic model, where the model is the component of  the large language model." .


###  https://w3id.org/peo#has_prompt
:has_prompt rdf:type owl:ObjectProperty ;
            rdfs:domain :chat ;
            rdfs:range :prompt ;
            rdfs:label "has_prompt" ;
            :description "Relation between chat and a prompt, where the chat has that prompt." .


###  https://w3id.org/peo#has_response
:has_response rdf:type owl:ObjectProperty ;
              owl:inverseOf :is_response_of ;
              rdfs:domain :chat ;
              rdfs:range :response ;
              rdfs:label "has_response" ;
              :description "Relation between a chat and a response, where the chat has that response." .


###  https://w3id.org/peo#is_response_of
:is_response_of rdf:type owl:ObjectProperty ;
                rdfs:domain :response ;
                rdfs:range :chat ;
                rdfs:label "is_response_of" ;
                :description "Response belongs to a chat" .


###  https://w3id.org/peo#is_used_in_chat
:is_used_in_chat rdf:type owl:ObjectProperty ;
                 owl:inverseOf :uses_model ;
                 rdfs:domain :large_language_model ;
                 rdfs:range :chat ;
                 rdfs:label "is_used_in_chat" ;
                 :description "Relation between a large language model and a chat, where the model is used in that chat" .


###  https://w3id.org/peo#is_used_in_prompt
:is_used_in_prompt rdf:type owl:ObjectProperty ;
                   owl:inverseOf :prompt_generated_using ;
                   rdfs:domain :prompting_technique ;
                   rdfs:range :prompt ;
                   rdfs:label "is_used_in_prompt" ;
                   :description "Relation between a prompt and a prompting technique, where the prompt is generated using that prompting technique" .


###  https://w3id.org/peo#processed_by
:processed_by rdf:type owl:ObjectProperty ;
              owl:inverseOf :processes ;
              rdfs:domain :prompt ;
              rdfs:range :large_language_model ;
              rdfs:label "processed_by" ;
              :description "Relation between a prompt and a large language model, where the prompt is given as input to the large language model" .


###  https://w3id.org/peo#processes
:processes rdf:type owl:ObjectProperty ;
           rdfs:domain :large_language_model ;
           rdfs:range :prompt ;
           rdfs:label "processes" ;
           :description "Relation between a large language model and a prompt, where the large language model processes the prompt" .


###  https://w3id.org/peo#prompt_followedby_prompt
:prompt_followedby_prompt rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf owl:topObjectProperty ;
                          owl:inverseOf :prompt_follows_prompt ;
                          rdf:type owl:TransitiveProperty ;
                          rdfs:domain :prompt ;
                          rdfs:range :prompt ;
                          rdfs:label "prompt_followedby_prompt" ;
                          :description "The first prompt is after the second prompt" .


###  https://w3id.org/peo#prompt_followedby_response
:prompt_followedby_response rdf:type owl:ObjectProperty ;
                            owl:inverseOf :response_follows_prompt ;
                            rdf:type owl:TransitiveProperty ;
                            rdfs:domain :prompt ;
                            rdfs:range :response ;
                            rdfs:label "prompt_followedby_response" ;
                            :description "Prompt is after response" .


###  https://w3id.org/peo#prompt_follows_prompt
:prompt_follows_prompt rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf owl:topObjectProperty ;
                       rdf:type owl:TransitiveProperty ;
                       rdfs:domain :prompt ;
                       rdfs:range :prompt ;
                       rdfs:label "prompt_follows_prompt" ;
                       :description "The second prompt is after the first prompt" .


###  https://w3id.org/peo#prompt_follows_response
:prompt_follows_response rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf owl:topObjectProperty ;
                         owl:inverseOf :response_followedby_prompt ;
                         rdf:type owl:TransitiveProperty ;
                         rdfs:domain :prompt ;
                         rdfs:range :response ;
                         rdfs:label "prompt_follows_response" ;
                         :description "Response is after prompt" .


###  https://w3id.org/peo#prompt_generated_using
:prompt_generated_using rdf:type owl:ObjectProperty ;
                        rdfs:domain :prompt ;
                        rdfs:range :prompting_technique ;
                        rdfs:label "prompt_generated_using" ;
                        :description "Prompt generated using a prompting technique" .


###  https://w3id.org/peo#response_followedby_prompt
:response_followedby_prompt rdf:type owl:ObjectProperty ;
                            rdfs:subPropertyOf owl:topObjectProperty ;
                            rdf:type owl:TransitiveProperty ;
                            rdfs:domain :response ;
                            rdfs:range :prompt ;
                            rdfs:label "response_followedby_prompt" ;
                            :description "Response is after a prompt" .


###  https://w3id.org/peo#response_follows_prompt
:response_follows_prompt rdf:type owl:ObjectProperty ,
                                  owl:TransitiveProperty ;
                         rdfs:domain :response ;
                         rdfs:range :prompt ;
                         rdfs:label "response_follows_prompt" ;
                         :description "Prompt is after response" .


###  https://w3id.org/peo#response_generated_using
:response_generated_using rdf:type owl:ObjectProperty ;
                          rdfs:domain :response ;
                          rdfs:range :large_language_model ;
                          rdfs:label "response_generated_using" ;
                          :description "Response generated using a large language model" .


###  https://w3id.org/peo#solved_by
:solved_by rdf:type owl:ObjectProperty ;
           owl:inverseOf :solves_task ;
           rdfs:domain :task ;
           rdfs:range :prompting_technique ;
           rdfs:label "solved_by" ;
           :description "Task solved by a prompting technique" .


###  https://w3id.org/peo#solves_task
:solves_task rdf:type owl:ObjectProperty ;
             rdfs:domain :prompting_technique ;
             rdfs:range :task ;
             rdfs:label "solves_task" ;
             :description "Relation between a prompting technique and a task, where the technique solves that task." .


###  https://w3id.org/peo#uses_model
:uses_model rdf:type owl:ObjectProperty ;
            rdfs:domain :chat ;
            rdfs:range :large_language_model ;
            rdfs:label "uses_model" ;
            :description "Realtion between a chat and a large language model, where the chat is with the large language model" .


#################################################################
#    Data properties
#################################################################

###  https://w3id.org/peo#end_time_chat
:end_time_chat rdf:type owl:DatatypeProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain :chat ;
               rdfs:range xsd:string ;
               rdfs:label "end_time_chat" ;
               :description "End time of a chat" .


###  https://w3id.org/peo#has_body
:has_body rdf:type owl:DatatypeProperty ,
                   owl:FunctionalProperty ;
          rdfs:domain :prompt ;
          rdfs:range xsd:string ;
          rdfs:label "has_instruction" ;
          :description "Instruction of a prompt" .


###  https://w3id.org/peo#has_chat_title
:has_chat_title rdf:type owl:DatatypeProperty ,
                         owl:FunctionalProperty ;
                rdfs:label "has_chat_title" ;
                :description "Title of a chat" .


###  https://w3id.org/peo#has_description
:has_description rdf:type owl:DatatypeProperty ;
                 rdfs:domain :task ;
                 rdfs:range xsd:string ;
                 rdfs:label "has_description" ;
                 :description "Description of a specific task" .


###  https://w3id.org/peo#has_input_data
:has_input_data rdf:type owl:DatatypeProperty ;
                rdfs:domain :prompt ;
                rdfs:range xsd:string ;
                rdfs:label "has_input_data" ;
                :description "Input data of a prompt" .


###  https://w3id.org/peo#has_number_parameters
:has_number_parameters rdf:type owl:DatatypeProperty ;
                       rdfs:subPropertyOf owl:topDataProperty ;
                       rdf:type owl:FunctionalProperty ;
                       rdfs:domain :large_language_model ;
                       rdfs:range xsd:long ;
                       rdfs:label "has_number_parameters" ;
                       :description "Number of parameters of a large language model." .


###  https://w3id.org/peo#has_output_indicator
:has_output_indicator rdf:type owl:DatatypeProperty ;
                      rdfs:domain :prompt ;
                      rdfs:range xsd:string ;
                      rdfs:label "has_output_indicator" ;
                      :description "Output format of a prompt" .


###  https://w3id.org/peo#has_release_year
:has_release_year rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf owl:topDataProperty ;
                  rdf:type owl:FunctionalProperty ;
                  rdfs:domain :large_language_model ;
                  rdfs:range xsd:integer ;
                  rdfs:label "has_release_year" ;
                  :description "Release year of a model or a model architecture." .


###  https://w3id.org/peo#has_response_value
:has_response_value rdf:type owl:DatatypeProperty ,
                             owl:FunctionalProperty ;
                    rdfs:domain :response ;
                    rdfs:range xsd:string ;
                    rdfs:label "has_response_value" ;
                    :description "Actual value of a response" .


###  https://w3id.org/peo#start_time_chat
:start_time_chat rdf:type owl:DatatypeProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain :chat ;
                 rdfs:range xsd:string ;
                 rdfs:label "start_time_chat" ;
                 :description "Start time of a chat" .


#################################################################
#    Classes
#################################################################

###  https://w3id.org/peo#Alpaca
:Alpaca rdf:type owl:Class ;
        rdfs:subClassOf :large_language_model ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :based_on ;
                          owl:someValuesFrom :LLaMA
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :has_capability ;
                          owl:someValuesFrom :text_processing
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :has_model_architecture ;
                          owl:someValuesFrom :transformer
                        ] ;
        rdfs:label "Alpaca" ;
        :description "Fine-tuned language model for instruction-following tasks based on Meta's LLaMA model." ;
        :paper "n.a." ;
        :website "https://crfm.stanford.edu/2023/03/13/alpaca.html" .


###  https://w3id.org/peo#Audio_task
:Audio_task rdf:type owl:Class ;
            rdfs:subClassOf :task ;
            rdfs:label "Audio task" ;
            :description "Task involving audio processing" .


###  https://w3id.org/peo#BERT
:BERT rdf:type owl:Class ;
      rdfs:subClassOf :large_language_model ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :has_capability ;
                        owl:someValuesFrom :text_processing
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :has_model_architecture ;
                        owl:someValuesFrom :Encoder_only
                      ] ;
      rdfs:label "BERT" ;
      :description "(Bidirectional Encoder Representations from Transformers) An LLM by Google designed for NLP tasks, known for understanding context in both directions within text." ;
      :paper "https://aclanthology.org/N19-1423.pdf" ;
      :website "https://huggingface.co/bert-base-uncased" .


###  https://w3id.org/peo#BLIP-2
:BLIP-2 rdf:type owl:Class ;
        rdfs:subClassOf :large_language_model ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :has_capability ;
                          owl:someValuesFrom :image_processing
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :has_model_architecture ;
                          owl:someValuesFrom :Q-Former
                        ] ;
        rdfs:label "BLIP-2" ;
        :description "Multimodal model that integrates a frozen image encoder with a large language model via a Querying Transformer, enabling tasks like image captioning and visual question answering." ;
        :paper "https://arxiv.org/abs/2301.12597" ;
        :website "https://huggingface.co/docs/transformers/main/model_doc/blip-2" .


###  https://w3id.org/peo#BLOOM
:BLOOM rdf:type owl:Class ;
       rdfs:subClassOf :large_language_model ,
