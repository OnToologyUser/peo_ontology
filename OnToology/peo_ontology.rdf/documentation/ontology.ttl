          rdfs:subPropertyOf owl:topObjectProperty ;
          owl:inverseOf :basis_for ;
          rdfs:domain :large_language_model ;
          rdfs:range :large_language_model ;
          rdfs:label "based_on" ;
          :description "Relationship between to two large language models in which a large language model uses another large language model as base." .


