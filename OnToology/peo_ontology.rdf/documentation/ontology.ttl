###  https://w3id.org/peo#bert
:bert rdf:type owl:Class ;
      rdfs:subClassOf :large_language_model ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :has_capability ;
                        owl:someValuesFrom :text_processing
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :has_model_architecture ;
                        owl:someValuesFrom :encoder_only
                      ] ;
      rdfs:label "BERT" ;
      :description "(Bidirectional Encoder Representations from Transformers) An LLM by Google designed for NLP tasks, known for understanding context in both directions within text." ;
      :paper "https://aclanthology.org/N19-1423.pdf" ;
      :website "https://huggingface.co/bert-base-uncased" .


###  https://w3id.org/peo#blip-2
:blip-2 rdf:type owl:Class ;
        rdfs:subClassOf :large_language_model ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :has_capability ;
                          owl:someValuesFrom :image_processing
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :has_model_architecture ;
                          owl:someValuesFrom :q_former
                        ] ;
        rdfs:label "BLIP-2" ;
        :description "Multimodal model that integrates a frozen image encoder with a large language model via a Querying Transformer, enabling tasks like image captioning and visual question answering." ;
        :paper "https://arxiv.org/abs/2301.12597" ;
        :website "https://huggingface.co/docs/transformers/main/model_doc/blip-2" .


