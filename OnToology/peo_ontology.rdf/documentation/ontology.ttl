###  https://w3id.org/peo#automatic_cot_prompting
:automatic_cot_prompting rdf:type owl:Class ;
                         rdfs:subClassOf :prompting_technique ;
                         rdfs:label "Automatic Chain-of-Thought prompting" ;
                         :description "Technique where models autonomously generate step-by-step reasoning, enhancing structured problem-solving without explicit intermediate prompts." ;
                         :reference "https://arxiv.org/abs/2210.03493" .


###  https://w3id.org/peo#base_model
:base_model rdf:type owl:Class ;
            rdfs:label "Base model" ;
            :description "Base model used as base architecture by each large language model." .


###  https://w3id.org/peo#bert
:bert rdf:type owl:Class ;
      rdfs:subClassOf :large_language_model ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :has_capability ;
                        owl:someValuesFrom :text_processing
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :has_model_architecture ;
                        owl:someValuesFrom :encoder_only
                      ] ;
      rdfs:label "BERT" ;
      :description "(Bidirectional Encoder Representations from Transformers) An LLM by Google designed for NLP tasks, known for understanding context in both directions within text." ;
      :paper "https://aclanthology.org/N19-1423.pdf" ;
      :website "https://huggingface.co/bert-base-uncased" .


###  https://w3id.org/peo#blip-2
:blip-2 rdf:type owl:Class ;
        rdfs:subClassOf :large_language_model ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :has_capability ;
                          owl:someValuesFrom :image_processing
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :has_model_architecture ;
                          owl:someValuesFrom :q_former
                        ] ;
        rdfs:label "BLIP-2" ;
        :description "Multimodal model that integrates a frozen image encoder with a large language model via a Querying Transformer, enabling tasks like image captioning and visual question answering." ;
        :paper "https://arxiv.org/abs/2301.12597" ;
        :website "https://huggingface.co/docs/transformers/main/model_doc/blip-2" .


###  https://w3id.org/peo#bloom
:bloom rdf:type owl:Class ;
       rdfs:subClassOf :large_language_model ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :has_capability ;
                         owl:someValuesFrom :text_processing
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :has_model_architecture ;
                         owl:someValuesFrom :transformer
                       ] ;
       rdfs:label "BLOOM" ;
       :description "Large open-access language model designed to support multiple languages and dialects, aiming to make natural language processing more inclusive and accessible across diverse linguistic communities." ;
       :paper "https://arxiv.org/abs/2211.05100" ;
       :website "https://huggingface.co/bigscience/bloom" .


###  https://w3id.org/peo#capability
:capability rdf:type owl:Class ;
            rdfs:label "Capability" ;
            :description "Possibile capability of large language models." .


###  https://w3id.org/peo#chain_of_code_prompting
:chain_of_code_prompting rdf:type owl:Class ;
                         rdfs:subClassOf :code_prompting ;
                         rdfs:label "Chain-of-Code prompting" ;
                         :description "Technique that enhances reasoning in language models by combining code generation with a \"LMulator\" that simulates code execution for complex tasks, enabling models to solve both algorithmic and semantic problems." ;
                         :reference "https://arxiv.org/abs/2312.04474" .


###  https://w3id.org/peo#chain_of_knowledge_prompting
:chain_of_knowledge_prompting rdf:type owl:Class ;
                              rdfs:subClassOf :prompting_technique ;
                              rdfs:label "Chain-of-Knowledge prompting" ;
                              :description "Technique that enhances large language models by dynamically integrating factual information from heterogeneous knowledge sources, aiming to improve accuracy and reduce hallucinations in generated responses." ;
                              :reference "https://arxiv.org/abs/2305.13269" .


###  https://w3id.org/peo#chain_of_note_prompting
:chain_of_note_prompting rdf:type owl:Class ;
                         rdfs:subClassOf :prompting_technique ;
