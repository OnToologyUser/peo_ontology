#################################################################
#    Classes
#################################################################

###  https://w3id.org/peo#active_prompting
:active_prompting rdf:type owl:Class ;
                  rdfs:subClassOf :prompting_technique ;
                  rdfs:label "Active prompting" ;
                  :description "Technique where specific, dynamic instructions are given to a model to guide it through a multi-step or interactive process, often requiring the model to actively engage, reflect, or respond iteratively to achieve a targeted outcome." ;
                  :reference "https://arxiv.org/abs/2302.12246" .


###  https://w3id.org/peo#alpaca
:alpaca rdf:type owl:Class ;
        rdfs:subClassOf :large_language_model ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :based_on ;
                          owl:someValuesFrom :llama
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :has_capability ;
                          owl:someValuesFrom :text_processing
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :has_model_architecture ;
                          owl:someValuesFrom :transformer
                        ] ;
        rdfs:label "Alpaca" ;
        :description "Fine-tuned language model for instruction-following tasks based on Meta's LLaMA model." ;
        :paper "n.a." ;
        :website "https://crfm.stanford.edu/2023/03/13/alpaca.html" .


###  https://w3id.org/peo#analogical_prompting
:analogical_prompting rdf:type owl:Class ;
                      rdfs:subClassOf :prompting_technique ;
                      rdfs:label "Analogical prompting" ;
                      :description "Technique that guides large language models to self-generate relevant examples or knowledge based on analogies, aiding them in reasoning through new problems without labeled exemplars." ;
                      :reference "https://arxiv.org/pdf/2310.01714" .


